                <path d="M0,0 L115,115 L130,115 L142,142 L250,250 L250,0 Z"></path>
                <path
                    d="M128.3,109.0 C113.8,99.7 119.0,89.6 119.0,89.6 C122.0,82.7 120.5,78.6 120.5,78.6 C119.2,72.0 123.4,76.3 123.4,76.3 C127.3,80.9 125.5,87.3 125.5,87.3 C122.9,97.6 130.6,101.9 134.4,103.2"
                    fill="currentColor" style="transform-origin: 130px 106px;" class="octo-arm"></path>
                <path
                    d="M115.0,115.0 C114.9,115.1 118.7,116.5 119.8,115.4 L133.7,101.6 C136.9,99.2 139.9,98.4 142.2,98.6 C133.8,88.0 127.5,74.4 143.8,58.0 C148.5,53.4 154.0,51.2 159.7,51.0 C160.3,49.4 163.2,43.6 171.4,40.1 C171.4,40.1 176.1,42.5 178.8,56.2 C183.1,58.6 187.2,61.8 190.9,65.4 C194.5,69.0 197.7,73.2 200.1,77.6 C213.8,80.2 216.3,84.9 216.3,84.9 C212.7,93.1 206.9,96.0 205.4,96.6 C205.1,102.4 203.0,107.8 198.3,112.5 C181.9,128.9 168.3,122.5 157.7,114.1 C157.9,116.9 156.7,120.9 152.7,124.9 L141.0,136.5 C139.8,137.7 141.6,141.9 141.8,141.8 Z"
                    fill="currentColor" class="octo-body">

                </path>
            </svg>

        </a>
    </div>




    <% if (theme.avatar.url) { %>
        <a id="avatar" href="<%- config.root %>"><img id="avatar img" src="<%- theme.avatar.url %>"
                alt="<%- config.title%>" /></a>
        <% } %>
            <h1><a id="author" href="<%- config.root %>">
                    <%- config.author %>
                </a></h1>
            <% if(theme.Social_Accounts.github_follow) { -%>
                <div id="github-iframe">
                    <iframe src="<%- theme.Social_Accounts.github_follow.href %>" scrolling="no" frameborder="0"
                        width="150px" height="20px"></iframe>
                </div>
                <% } -%>

                    <% if (theme.Social_Accounts.weibostar) { -%>
                        <div id="weibo"></div>
                        <script src="https://tjs.sjs.sinajs.cn/open/api/js/wb.js" type="text/javascript"
                            charset="utf-8"></script>
                        <%- theme.Social_Accounts.weibostar.code %>
                            <% } -%>

                                <%- partial('./follow') %>

                                    <span id="slogan">
                                        <% if (theme.slogan) { %>
                                            Slogan:
                                            <ul>
                                                <% for ( sub in theme.slogan ) { -%>
                                                    <li style="display: none;">
                                                        <%- theme.slogan[sub] %>
                                                    </li>
                                                    <% } -%>
                                            </ul>
                                            <% } -%>
                                                <script>
                                                    var sel = $('#slogan > ul > li');
                                                    //console.log(sel[Math.floor(Math.random()*sel.length)].style)//
                                                    function lunbo() {
                                                        arguments.callee.pre = arguments.callee.pre || 0;
                                                        sel[arguments.callee.pre].style.display = "none";
                                                        arguments.callee.pre = Math.floor(Math.random() * sel.length);
                                                        sel[arguments.callee.pre].style.display = "inline-block";
                                                        setTimeout(lunbo, 30000);
                                                    }
                                                    lunbo();
                                                </script>
                                    </span>

                                    <span id="donate">
                                        <a href="<%- config.root %>donate" title="<%- theme.reward.comment %>">Donate &
                                            Reward </a>
                                    </span>

                                    <footer>
                                        <section id="footer-message">Copyright &copy;
                                            <% if(theme.copyright.since ) { -%>
                                                <%- theme.copyright.since %> -
                                                    <% } -%>
                                                        <span id="year">2021</span>
                                                        <a href="https://yaoqs.github.io" target="_blank">LordYao</a>.
                                                        <% if (theme.copyright.creative_commons.url){-%>
                                                            <img src="<%- theme.copyright.creative_commons.url %>" />
                                                            <% } else { %>
                                                                <%- theme.copyright.creative_commons.license %> .
                                                                    <%} %>
                                                                        All Rights Reserved. <br />
                                                                        Powered by <a href="http://hexo.io/"
                                                                            target="_blank">Hexo</a> & <a
                                                                            href="https://pages.github.com/"
                                                                            target="_blank">Github
                                                                            Pages</a>.
                                                                        Theme <a
                                                                            href="https://github.com/yaoqs/hexo-theme-scholar-future"
                                                                            target="_blank">
                                                                            <%- theme.name %>
                                                                        </a> designed by <a
                                                                            href="https://yaoqs.github.io"
                                                                            target="_blank">
                                                                            LordYao
                                                                        </a>.
                                        </section>
                                        <script>
                                            var year = new Date(Date.now()).getFullYear();
                                            document.getElementById("year").innerText = year;
                                        </script>
                                        <%- partial('./Statistics') %>

</header>